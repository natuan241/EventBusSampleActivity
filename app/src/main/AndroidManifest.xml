<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.natuan.eventbussampleactivity" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="org.natuan.eventbussampleactivity.ActivityPublisher"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.natuan.eventbussampleactivity.ActivitySubscriber"
            android:label="@string/title_activity_activity_subscriber" >
        </activity>
        <activity
            android:name="org.natuan.eventbussampleactivity.BaseActivity"
            android:label="@string/title_activity_base" >
        </activity>
    </application>

</manifest>
